{% extends 'index.html.twig' %}
{% block body %}

<div class="row row_first">
        <div class="col-md-1"></div>
        
    </div>

    <div class="row display_img">
        <div class="row">
            <div class="col-lg-12">
                <div class="col-lg-2"></div>
                <h1 class="col-lg-8 services_title">
                    {% block heading %}{% endblock %}
                </h1>
            </div>
        </div>
